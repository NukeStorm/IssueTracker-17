{
    "plugins": [
            "prettier"
    ],
    "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "google",
            "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "ignorePatterns": [
            "node_modules/"
    ],

    "rules": {
        "no-console": "error",
        "camelcase": "error",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "require-jsdoc": 0
    }
}