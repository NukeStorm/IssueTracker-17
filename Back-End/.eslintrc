{
    "plugins": [
            "prettier"
    ],
    "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "env": {
        "browser": false,
        "node": true
    },
    "ignorePatterns": [
            "node_modules/"
    ],

    "rules": {
        "no-console": "error",
        "camelcase": "error",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}